    <script type="text/javascript">
    $(document).ready(function(){
        $('#hdr-new-container').addClass('responsiveHeader');
        var $navInner = $('#hdr-bottom'); //DOM element
        $navInner.load("./browseMenu.html", function() {
        
          var $browseMenu = $navInner.find('.browseMenu');
          $browseMenu.addClass('browseMenuExpanded');

            handleFlyoutHover($browseMenu); //only if not an iPhone

            handleFlyoutClick($browseMenu);
           $(".mainMenu").css('transform','translate3d(0,0,0)');
            $("#leftNavRdesignBtn").css('transform','translate3d(0,0,0)');
          
        
        });
        var $hdr_top_nav = $('.hdr_top_nav'); //DOM container of nav elements
  var $hamburgerLI = $('<li class="top_nav_hamburger"/>');
  $hamburgerLI.insertAfter($hdr_top_nav.find('.top_nav_saved'));
  var handlingHamburgerInteraction = false;

function handleFlyoutClick($e) { //$e is the browseMenu root
  var majorBreakpointWidth = 1024;

  var $window = $(window);
  var $html = $('html');
  var $top_nav_hamburger = $('.top_nav_hamburger');
  var $flyoutLinks = $e.find('.flyoutLink');


  function openBurger() {
    handlingHamburgerInteraction = true;
    $html.addClass('forceCondensedHamburgerState');
    var w = $(window).width();
    var isNarrowView = w < majorBreakpointWidth;
    var layoutClass = (isNarrowView) ? 'browseMenuCollapsed' : 'browseMenuExpanded';
    $e.attr('class', 'browseMenu ' + layoutClass + ' forceCondensedHamburgerState');
  }


  function closeBurger() {
    handlingHamburgerInteraction = false;
    $e.attr('class', 'browseMenu browseMenuExpanded');
    $html.removeClass('forceCondensedHamburgerState');
  }

  $html.click(function(evt) {
    if (handlingHamburgerInteraction && !/top_nav_hamburger/.test(evt.target.className))
      closeBurger();
  });


  $top_nav_hamburger.click(function() {

    if (!handlingHamburgerInteraction) {
      openBurger();
    } else {
      closeBurger();
    }

  });
}

function handleFlyoutHover($e) { //$e is the browseMenu root
  var $window = $(window);
  var $html = $('html');
  var $flyoutLinks = $e.find('.flyoutLink');


  function closeMenu(evt) {
    if (handlingHamburgerInteraction) return;
    $e.attr('class', 'browseMenu browseMenuExpanded');
    $html.removeClass('forceMainMenuExpanded');
  }


  function openMenu(evt) {
    if (handlingHamburgerInteraction) return;

    // if the page is scrolled at all this wont work 
    // because of css on html element ==> html.forceMainMenuExpanded{overflow: hidden;}
    // skip if there is preexisting page scroll
    if ($window.scrollTop() > 0) return;

    var targetId = evt.currentTarget.classList[1];

    if (/mouseenter/.test(evt.type)) {
      var w = $(window).width();
      var isNarrowView = w < 1024;
      var layoutClass = (isNarrowView) ? 'browseMenuCollapsed' : 'browseMenuExpanded';
      // $e.attr('class','browseMenu ' + layoutClass + ' '+targetId+'Selected blurNav');
      $e.attr('class', 'browseMenu ' + layoutClass + ' ' + targetId + 'Selected forceMainMenuExpanded');

      if (isNarrowView)
        $html.addClass('forceMainMenuExpanded');
    }

  }



  $e.on('mouseleave', function(evt) {
    closeMenu(evt);
  });

  $e.on('mouseenter', '.flyoutLink', function(evt) {
    openMenu(evt);
  });
}


    });
  


  </script>
